<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="getUTCFullYear" parent="Date" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>getUTCFullYear – Get the year, according to universal time (four digits).</p>
	</lead>
	<description>
		<p>Date.getUTCFullYear(<params/>)</p>
	</description>
	<return>
		<p>Integer</p>
	</return>
	<examples>
		<example>
			<heading>getUTCFullYear example</heading>
			<code>
				console.log(new Date().getUTCFullYear());  // Gets the current year, according to UTC.
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Date/getUTCFullYear">Date.getUTCFullYear method</mdn>
	</external>

</method>
