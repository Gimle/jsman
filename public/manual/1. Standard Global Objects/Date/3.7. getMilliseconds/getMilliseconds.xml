<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="getMilliseconds" parent="Date">
	<lead>
		<p>getMilliseconds – Get the milliseconds, according to local time (0–999).</p>
	</lead>
	<description>
		<p>Date.getMilliseconds(<params/>)</p>
	</description>
	<return>
		<p>Integer</p>
	</return>
	<examples>
		<example>
			<heading>getMilliseconds example</heading>
			<code>
				var date = new Date();
				console.log(date.getMilliseconds());  // Logs the current milliseconds.
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Date/getMilliseconds">Date.getMilliseconds method</mdn>
	</external>

</method>
