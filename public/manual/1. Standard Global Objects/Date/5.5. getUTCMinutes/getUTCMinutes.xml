<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="getUTCMinutes" parent="Date">
	<lead>
		<p>getUTCMinutes – Get the minutes, according to universal time (0–59).</p>
	</lead>
	<description>
		<p>Date.getUTCMinutes(<params/>)</p>
	</description>
	<return>
		<p>Integer</p>
	</return>
	<examples>
		<example>
			<heading>getUTCMinutes example</heading>
			<code>
				console.log(new Date().getUTCMinutes());  // Gets the current minute, according to UTC.
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Date/getUTCMinutes">Date.getUTCMinutes method</mdn>
	</external>

</method>
