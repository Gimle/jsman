<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="setUTCFullYear" parent="Date" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>setUTCFullYear – Set the year, according to universal time (four digits).</p>
	</lead>
	<description>
		<p>Date.setUTCFullYear(<params/>)</p>
	</description>
	<parameters>
		<parameter name="year" type="integer" optional="false"></parameter>
	</parameters>
	<return>
		<p>Integer. The new timestamp.</p>
	</return>
	<examples>
		<example>
			<heading>setUTCFullYear example</heading>
			<code>
				var date = new Date(Date.UTC(2015, 9, 21, 16, 29, 00));
				console.log(date.getTime());             // 1445444940000

				console.log(date.setUTCFullYear(2000));  // 972145740000
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Date/setUTCFullYear">Date.setUTCFullYear method</mdn>
	</external>
</method>
