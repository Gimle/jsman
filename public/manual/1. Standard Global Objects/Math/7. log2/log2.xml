<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="log2" parent="Math" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>log2 â€” Returns the base 2 logarithm of a number.</p>
	</lead>
	<description>
		<p>Math.log2(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>log2 example</heading>
			<code>
				console.log(Math.log2(3));    // 1.584962500721156
				console.log(Math.log2(8));    // 3
				console.log(Math.log2(-3));   // NaN
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/log2">Math.log2 method</mdn>
	</external>

</method>
