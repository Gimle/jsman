<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="max" parent="Math">
	<lead>
		<p>max — Returns the number with the highest value.</p>
	</lead>
	<description>
		<p>Math.max(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="true"></parameter>
		<parameter name="numbers, …" type="number" optional="true"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>max example</heading>
			<code>
				console.log(Math.max(11, 8));   // 11
				console.log(Math.max(-2, 3));   // 3
				console.log(Math.max(-3, -7));  // -3
			</code>
		</example>
	</examples>
	<see-also>
		<ul>
			<li><xref href="manual/1. Standard Global Objects/Math/3.1. min/min.xml">max</xref></li>
		</ul>
	</see-also>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/max">Math.max method</mdn>
	</external>

</method>
