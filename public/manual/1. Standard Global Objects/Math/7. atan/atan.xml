<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="atan" parent="Math" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>atan â€” Returns the arc-tangent (in radians) of a number.</p>
	</lead>
	<description>
		<p>Math.atan(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>atan example</heading>
			<code>
				console.log(Math.atan(1));    // 0.7853981633974483
				console.log(Math.atan(4));    // 1.3258176636680326
				console.log(Math.atan(4.2));  // 1.3370531459259951
				console.log(Math.atan(10));   // 1.4711276743037347
				console.log(Math.atan(0));    // 0
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/atan">Math.atan method</mdn>
	</external>

</method>
