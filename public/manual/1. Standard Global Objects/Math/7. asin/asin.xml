<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="asin" parent="Math" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>asin â€” Returns the arc-sine (in radians) of a number.</p>
	</lead>
	<description>
		<p>Math.asin(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false">A number between -1 and 1.</parameter>
	</parameters>
	<examples>
		<example>
			<heading>asin example</heading>
			<code>
				console.log(Math.asin(0.5));   // 0.5235987755982989
				console.log(Math.asin(4));     // NaN
				console.log(Math.asin(1));     // 1.5707963267948966
				console.log(Math.asin(-1));    // -1.5707963267948966
				console.log(Math.asin(0));     // 0
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/asin">Math.asin method</mdn>
	</external>

</method>
