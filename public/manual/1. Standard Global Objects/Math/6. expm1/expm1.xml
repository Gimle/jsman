<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="expm1" parent="Math" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>expm1 â€” Returns <xref href="manual/1. Standard Global Objects/Math/1. E/E.xml">Math.E</xref> raised to a power - 1.</p>
	</lead>
	<description>
		<p>Math.expm1(<params/>)</p>
	</description>
	<parameters>
		<parameter name="power" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>expm1 example</heading>
			<code>
				console.log(Math.expm1(0));     // 0
				console.log(Math.expm1(0.5));   // 0.6487212707001282
				console.log(Math.expm1(1));     // 1.718281828459045
				console.log(Math.expm1(-1));    // -0.6321205588285577
				console.log(Math.expm1(3));     // 19.085536923187668
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/expm1">Math.expm1 method</mdn>
	</external>

</method>
