<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="ceil" parent="Math">
	<lead>
		<p>ceil â€” Rounds up to the closest whole number.</p>
	</lead>
	<description>
		<p>Math.ceil(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>ceil example</heading>
			<code>
				console.log(Math.ceil(1));     // 1
				console.log(Math.ceil(1.0));   // 1
				console.log(Math.ceil(1.2));   // 2
				console.log(Math.ceil(1.49));  // 2
				console.log(Math.ceil(1.5));   // 2
				console.log(Math.ceil(1.8));   // 2
				console.log(Math.ceil(2));     // 2
			</code>
		</example>

	</examples>
	<see-also>
		<ul>
			<li><xref href="manual/1. Standard Global Objects/Math/2.3. floor/floor.xml">floor</xref></li>
			<li><xref href="manual/1. Standard Global Objects/Math/2.1. round/round.xml">round</xref></li>
		</ul>
	</see-also>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/ceil">Math.ceil method</mdn>
	</external>

</method>
