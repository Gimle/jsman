<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="asinh" parent="Math">
	<lead>
		<p>asinh â€” Returns the arc-sine (in radians) of a number.</p>
	</lead>
	<description>
		<p>Math.asinh(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>asinh example</heading>
			<code>
				console.log(Math.asinh(1));    // 0.8813735870195429
				console.log(Math.asinh(4));    // 2.0947125472611012
				console.log(Math.asinh(4.2));  // 2.142111908236906
				console.log(Math.asinh(10));   // 2.99822295029797
				console.log(Math.asinh(0));    // 0
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/asinh">Math.asinh method</mdn>
	</external>

</method>
