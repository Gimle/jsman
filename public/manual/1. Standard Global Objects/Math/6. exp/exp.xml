<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="exp" parent="Math" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>exp â€” Returns <xref href="manual/1. Standard Global Objects/Math/1. E/E.xml">Math.E</xref> raised to a power.</p>
	</lead>
	<description>
		<p>Math.exp(<params/>)</p>
	</description>
	<parameters>
		<parameter name="power" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>exp example</heading>
			<code>
				console.log(Math.exp(0));     // 1
				console.log(Math.exp(0.5));   // 1.6487212707001282
				console.log(Math.exp(1));     // 2.718281828459045
				console.log(Math.exp(-1));    // 0.3678794411714424
				console.log(Math.exp(3));     // 20.085536923187668
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/exp">Math.exp method</mdn>
	</external>

</method>
