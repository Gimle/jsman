<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="sqrt" parent="Math">
	<lead>
		<p>sqrt â€” Returns the square root of a number.</p>
	</lead>
	<description>
		<p>Math.sqrt(<params/>)</p>
	</description>
	<parameters>
		<parameter name="number" type="number" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>sqrt example</heading>
			<code>
				console.log(Math.sqrt(4));    // 2
				console.log(Math.sqrt(0));    // 0
				console.log(Math.sqrt(-3));   // NaN
				console.log(Math.sqrt('2'));  // 1.4142135623730951
			</code>
		</example>

	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Math/sqrt">Math.sqrt method</mdn>
	</external>

</method>
