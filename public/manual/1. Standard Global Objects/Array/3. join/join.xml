<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="join" parent="Array" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>join â€” Joins the elements of an array into a string.</p>
	</lead>
	<description>
		<p>Array.join(<params/>)</p>
	</description>
	<parameters>
		<parameter name="separator" type="string" optional="true"></parameter>
	</parameters>
	<return>
		<p>string primitive.</p>
	</return>
	<examples>
		<example>
			<heading>join example</heading>
			<code>
				var array = [2, 4, 8, 16];
				console.log(array);            // [2, 4, 8, 16]
				console.log(array.join());     // 2,4,8,16
				console.log(array.join(';'));  // 2;4;8;16
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array/join">Array.join method</mdn>
	</external>

</method>
