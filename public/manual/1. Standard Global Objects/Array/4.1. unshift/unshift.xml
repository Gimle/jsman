<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="unshift" parent="Array" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../../schema/jsman-manual.xsd">
	<lead>
		<p>unshift â€” Adds a new value to the beginning of the array.</p>
	</lead>
	<description>
		<p>Array.unshift(<params/>)</p>
	</description>
	<parameters>
		<parameter name="value" type="mixed" optional="false"></parameter>
	</parameters>
	<return>
		<p>integer. The new length of the array.</p>
	</return>
	<examples>
		<example>
			<heading>unshift example</heading>
			<code>
				var array = [4, 8, 16, 32];
				console.log(array);              // [4, 8, 16, 32]
				console.log(array.length);       // 4
				console.log(array.unshift(2));   // 5
				console.log(array);              // [2, 4, 8, 16, 32]
				console.log(array.length);       // 5
			</code>
		</example>
	</examples>
	<see-also>
		<ul>
			<li><p><xref href="manual/1. Standard Global Objects/Array/4.2. push/push.xml">Array.push()</xref></p></li>
			<li><p><xref href="manual/1. Standard Global Objects/Array/4.3. shift/shift.xml">Array.shift()</xref></p></li>
			<li><p><xref href="manual/1. Standard Global Objects/Array/4.4. pop/pop.xml">Array.pop()</xref></p></li>
		</ul>
	</see-also>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array/unshift">Array.unshift method</mdn>
	</external>

</method>
