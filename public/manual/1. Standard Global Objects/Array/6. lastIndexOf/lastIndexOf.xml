<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="lastIndexOf" parent="Array">
	<lead>
		<p>lastIndexOf â€” Returns the last position of the element with the given value.</p>
	</lead>
	<description>
		<p>Array.lastIndexOf(<params/>)</p>
	</description>
	<parameters>
		<parameter name="search" type="integer" optional="true"></parameter>
		<parameter name="offset" type="integer" optional="true"></parameter>
	</parameters>
	<return>
		<p>integer.</p>
	</return>
	<examples>
		<example>
			<heading>lastIndexOf example</heading>
			<code>
				var array = [2, 4, 8, 4, 16, 4, 32];
				console.log(array.lastIndexOf());      // -1
				console.log(array.lastIndexOf(3));     // -1
				console.log(array.lastIndexOf(4));     // 5
				console.log(array.lastIndexOf(4, 2));  // 1
			</code>
		</example>
	</examples>
	<see-also>
		<ul>
			<li><xref href="manual/1. Standard Global Objects/Array/indexOf/indexOf.xml">Array.indexOf()</xref></li>
		</ul>
	</see-also>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array/lastIndexOf">Array.lastIndexOf method</mdn>
	</external>

</method>
