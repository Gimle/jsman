<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/methodWrapper.xsl"?>
<method name="push" parent="Array">
	<lead>
		<p>push â€” Adds a new value to the end of the array.</p>
	</lead>
	<description>
		<p>Array.push(<params/>)</p>
	</description>
	<parameters>
		<parameter name="value" type="mixed" optional="false"></parameter>
	</parameters>
	<return>
		<p>integer. The new length of the array.</p>
	</return>
	<examples>
		<example>
			<heading>push example</heading>
			<code>
				var array = [2, 4, 8, 16];
				console.log(array);            // [2, 4, 8, 16]
				console.log(array.length);     // 4
				console.log(array.push(32));   // 5
				console.log(array);            // [2, 4, 8, 16, 32]
				console.log(array.length);     // 5
			</code>
		</example>
	</examples>
	<see-also>
		<ul>
			<li><xref href="manual/1. Standard Global Objects/Array/pop/pop.xml">pop</xref></li>
			<li><xref href="manual/1. Standard Global Objects/Array/shift/shift.xml">shift</xref></li>
			<li><xref href="manual/1. Standard Global Objects/Array/unshift/unshift.xml">unshift</xref></li>
		</ul>
	</see-also>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array/push">Array.push method</mdn>
	</external>

</method>
