<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../../template/propertyWrapper.xsl"?>
<property name="length" parent="String">
	<lead>
		<p>length</p>
	</lead>
	<description>
		<p>String.length</p>
	</description>
	<return>
		integer.
	</return>
	<examples>
		<example>
			<heading>length example</heading>
			<code>
				console.log('Hello World'.length);  // 11
				console.log('हिं'.length);            // 3
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/String/length">String.length property</mdn>
	</external>

</property>
