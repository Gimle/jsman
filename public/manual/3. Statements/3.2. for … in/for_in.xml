<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../template/operatorWrapper.xsl"?>
<statement name="for … in" xmlns="http://www.jsman.net/xsd/manual"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jsman.net/xsd/manual ../../../schema/jsman-manual.xsd">
	<lead>
		<p>for … in</p>
	</lead>
	<description>
		<p>for (<params separator=" in"/>)</p>
	</description>
	<parameters>
		<parameter name="variable" type="mixed" optional="false"></parameter>
		<parameter name="object" type="object" optional="false"></parameter>
	</parameters>
	<examples>
		<example>
			<heading>for … in example</heading>
			<p>Iteration between two values.</p>
			<code>
				var obj = {foo: 'Foo', bar: 'Bar'};

				for (var prop in obj) {
					console.log(prop, obj[prop]);
				}
			</code>
		</example>
	</examples>
	<external>
		<mdn href="https://developer.mozilla.org/docs/Web/JavaScript/Reference/Statements/for...in">for … in statement</mdn>
	</external>
</statement>
